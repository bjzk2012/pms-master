package cn.kcyf.pms.core.enumerate;

public enum NoticeType {
    INFO("普通消息"),
    SYSTEM("系统消息"),
    BUSSINESS("业务消息"),
    WARN("警告消息"),
    TODO("待办事项");
    NoticeType(String message){
        this.message = message;
    }
    private String message;

    public String getMessage() {
        return message;
    }
}
